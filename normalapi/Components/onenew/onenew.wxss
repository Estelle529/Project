/* Components/onenew.wxss */
.onenew{
  padding: 0 20rpx;
  display: flex;
  height: 220rpx;
  background-color: #eee;
  margin-bottom: 15rpx;
  align-items: center;
}
.onenew image{
  width: 200rpx;
  height: 200rpx;
}
.right{
  height: 100%;
  margin-left: 30rpx;
}
.right-title{
  width: 460rpx;
  text-overflow: ellipsis;
  font-size: 35rpx;
  overflow: hidden;
  white-space: nowrap;
  margin-top: 50rpx;
  margin-bottom: 30rpx;
}
.right-create{
  font-size: 28rpx;
  color: #aaa;
}